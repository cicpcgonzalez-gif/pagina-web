      }
    },
    "android": {
      "package": "com.megarifas.app",
      "versionCode": 2,
      "permissions": [
        "INTERNET",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "VIBRATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.megarifas.app",
      "buildNumber": "1.0.1"
    },
    "owner": "megarifasapps-organization"
